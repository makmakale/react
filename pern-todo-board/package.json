{
  "name": "pern-todo-board",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "install-server": "cd backend && npm install",
    "start-server": "cd backend && node server.js",
    "start-dev-server": "cd backend && npm run server",
    "install-client": "cd frontend && npm install --production=false",
    "build-client": "cd frontend && npm run build",
    "start-client": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run start-dev-server\" \"npm run start-client\""
  },
  "keywords": [],
  "author": "Maksim Makarenko",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
